{
  "name": "react-grid-layout-demo",
  "description": "Interactive dashboard with React Grid Layout and Untitled UI components",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "author": "ipoly",
  "license": "MIT",
  "keywords": [
    "react",
    "grid-layout",
    "dashboard",
    "untitled-ui",
    "tailwind",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ipoly/react-grid-layout-demo.git"
  },
  "homepage": "https://ipoly.github.io/react-grid-layout-demo/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "style": "stylelint \"**/*.{css,scss}\"",
    "style:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@react-types/overlays": "^3.9.0",
    "@tailwindcss/vite": "^4.1.12",
    "@untitledui/icons": "^0.0.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-aria-components": "^1.11.0",
    "react-dom": "^19.1.1",
    "react-grid-layout": "^1.5.2",
    "react-resizable": "^3.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-resizable": "^3.0.8",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --max-warnings 0",
      "prettier --check"
    ],
    "*.{css,scss}": [
      "stylelint",
      "prettier --check"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --check"
    ]
  }
}
